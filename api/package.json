    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npm run start:watch",
        "start:watch": "npx prisma migrate dev && NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
        "migrate": "npx prisma migrate dev",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start:prod:migrate": "npx prisma migrate deploy && npm run start:prod",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "test:ci": "jest --ci --reporters=default --reporters=jest-junit && gitlab-ci-reporter",
        "test:ci-gitlab": "jest --ci --reporters=default --reporters=jest-junit"
    },
    "dependencies": {
